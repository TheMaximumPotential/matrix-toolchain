{
	"deploy": {
		"packages": [
			{
				"files": ["**/*"],
				"exclude": [
					"node_modules/**",
					".git/**",
					".vscode/**",
					"**/node_modules/**"
				],
				"deployOnSave": false
			}
		],
		"targets": [
			{
				"type": "sftp",
				"name": "", // 随便起
				"dir": "", // 服务器上存放项目的文件夹
				"host": "", // 服务器ip地址
				"port": 22,
				"user": "", // 用户名
				"privateKey": "" // 本地密钥位置
			}
		]
	}
}
